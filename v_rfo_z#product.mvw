create materialized view U1.V_RFO_Z#PRODUCT
build deferred
refresh complete on demand
as
select id,
       class_id,
       c_date_begin,
       c_date_begining,
       c_date_close,
       c_date_ending,
       c_create_user,
       c_notes,
       c_com_status,
       c_num_dog,
       state_id,
       sn,
       su
  from ibs.Z#PRODUCT@rfo_snap;
grant select on U1.V_RFO_Z#PRODUCT to LOADDB;


