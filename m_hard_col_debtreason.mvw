CREATE MATERIALIZED VIEW U1.M_HARD_COL_DEBTREASON
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "DEBTREASON"."ID" "ID","DEBTREASON"."CODE" "CODE","DEBTREASON"."ORD" "ORD","DEBTREASON"."VALUE" "VALUE" FROM "DEBTCOLL"."DEBTREASON"@"HCOL" "DEBTREASON";
grant select on U1.M_HARD_COL_DEBTREASON to LOADDB;


