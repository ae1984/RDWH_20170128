CREATE MATERIALIZED VIEW U1.M_HARD_COL_ASSETCATEGORY
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ASSETCATEGORY"."ID" "ID","ASSETCATEGORY"."DESCRIPTION" "DESCRIPTION","ASSETCATEGORY"."HASADDRESS" "HASADDRESS","ASSETCATEGORY"."NAME" "NAME","ASSETCATEGORY"."PARENT_ID" "PARENT_ID" FROM "DEBTCOLL"."ASSETCATEGORY"@"HCOL" "ASSETCATEGORY";
grant select on U1.M_HARD_COL_ASSETCATEGORY to LOADDB;


