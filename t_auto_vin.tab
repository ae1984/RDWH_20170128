  autovin_dwh_client_id          NUMBER not null,
  autovin_rbo_client_id          NUMBER,
  autovin_valid                  NUMBER
)
tablespace USERS_F
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 8M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
