CREATE MATERIALIZED VIEW U1.M_HARD_COL_ASSETADDRESS
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ASSETADDRESS"."ID" "ID","ASSETADDRESS"."FLAT" "FLAT","ASSETADDRESS"."HOUSE" "HOUSE","ASSETADDRESS"."POSTALCODE" "POSTALCODE","ASSETADDRESS"."STREET" "STREET","ASSETADDRESS"."SETTLEMENT_ID" "SETTLEMENT_ID","ASSETADDRESS"."STATE_ID" "STATE_ID" FROM "DEBTCOLL"."ASSETADDRESS"@"HCOL" "ASSETADDRESS";
grant select on U1.M_HARD_COL_ASSETADDRESS to LOADDB;


