CREATE MATERIALIZED VIEW U1.M_HARD_COL_ASSESSMENT
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ASSESSMENT"."ID" "ID","ASSESSMENT"."CREATEDDATE" "CREATEDDATE","ASSESSMENT"."MODIFIEDDATE" "MODIFIEDDATE","ASSESSMENT"."COSTAMOUNT" "COSTAMOUNT","ASSESSMENT"."COSTCURRENCY" "COSTCURRENCY","ASSESSMENT"."CREATEDON" "CREATEDON","ASSESSMENT"."DATEOFEXECUTION" "DATEOFEXECUTION","ASSESSMENT"."PREVIOUSCOSTAMOUNT" "PREVIOUSCOSTAMOUNT","ASSESSMENT"."PREVIOUSCOSTCURRENCY" "PREVIOUSCOSTCURRENCY","ASSESSMENT"."TYPE" "TYPE","ASSESSMENT"."AUTHOR_ID" "AUTHOR_ID","ASSESSMENT"."MODIFIEDBY_ID" "MODIFIEDBY_ID","ASSESSMENT"."INDEPENDENTCOMPANY_ID" "INDEPENDENTCOMPANY_ID","ASSESSMENT"."OWNING_SESSION_ID" "OWNING_SESSION_ID" FROM "DEBTCOLL"."ASSESSMENT"@"HCOL" "ASSESSMENT";
grant select on U1.M_HARD_COL_ASSESSMENT to LOADDB;


