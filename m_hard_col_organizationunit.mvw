CREATE MATERIALIZED VIEW U1.M_HARD_COL_ORGANIZATIONUNIT
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ORGANIZATIONUNIT"."ID" "ID","ORGANIZATIONUNIT"."CODE" "CODE","ORGANIZATIONUNIT"."NAME" "NAME","ORGANIZATIONUNIT"."PARENT_ID" "PARENT_ID","ORGANIZATIONUNIT"."GUID" "GUID","ORGANIZATIONUNIT"."ISREMOVED" "ISREMOVED","ORGANIZATIONUNIT"."EXTERNALCODE" "EXTERNALCODE","ORGANIZATIONUNIT"."ISBRANCH" "ISBRANCH","ORGANIZATIONUNIT"."LOCATION_ID" "LOCATION_ID" FROM "DEBTCOLL"."ORGANIZATIONUNIT"@"HCOL" "ORGANIZATIONUNIT";
grant select on U1.M_HARD_COL_ORGANIZATIONUNIT to LOADDB;


