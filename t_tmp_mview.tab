create table U1.T_TMP_MVIEW
(
  mview_name VARCHAR2(200 CHAR),
  mbool      NUMBER(1),
  owner      VARCHAR2(200 CHAR)
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.T_TMP_MVIEW to LOADDB;

