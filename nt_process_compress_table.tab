create table U1.NT_PROCESS_COMPRESS_TABLE
(
  sdt          DATE,
  id           NUMBER,
  table_name   VARCHAR2(128),
  mb           NUMBER,
  processed_dt DATE
)
tablespace USERS
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;

