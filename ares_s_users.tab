create table U1.ARES_S_USERS
(
  id        NUMBER not null,
  login     VARCHAR2(128),
  passwd    VARCHAR2(512),
  is_active NUMBER default 0
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.ARES_S_USERS to LOADDB;
grant select on U1.ARES_S_USERS to LOADER;

