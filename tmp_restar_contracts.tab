create table U1.TMP_RESTAR_CONTRACTS
(
  red                  VARCHAR2(50),
  pp                   VARCHAR2(50),
  contract_number      VARCHAR2(50),
  common_current_debts VARCHAR2(50),
  blue                 VARCHAR2(50)
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.TMP_RESTAR_CONTRACTS to LOADDB;
grant select on U1.TMP_RESTAR_CONTRACTS to LOADER;

