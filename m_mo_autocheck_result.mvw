CREATE MATERIALIZED VIEW U1.M_MO_AUTOCHECK_RESULT
REFRESH FORCE ON DEMAND
AS
SELECT "T"."REQUEST_ID" "REQUEST_ID","T"."RFO_CLIENT_ID" "RFO_CLIENT_ID","T"."FOLDER_ID" "FOLDER_ID","T"."VERIF_ID" "VERIF_ID","T"."AUTO_PHOTO_RESULT" "AUTO_PHOTO_RESULT","T"."VERIF_STEP" "VERIF_STEP","T"."SYSTEM_SOURCE" "SYSTEM_SOURCE","T"."STATUS" "STATUS","T"."DATE_CREATE" "DATE_CREATE","T"."DATE_SEND" "DATE_SEND","T"."ERROR_MSG" "ERROR_MSG" FROM "U1"."T_AUTOCHECK_RESULT"@"MO1_PROD" "T";
grant select on U1.M_MO_AUTOCHECK_RESULT to LOADDB;
grant select on U1.M_MO_AUTOCHECK_RESULT to LOADER;


