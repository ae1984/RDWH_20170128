create table U1.TMP_RYB_USERS
(
  user_name    VARCHAR2(200),
  iin          VARCHAR2(50),
  phone_mobile VARCHAR2(100),
  update_phone VARCHAR2(100)
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
