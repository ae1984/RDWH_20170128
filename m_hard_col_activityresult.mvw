CREATE MATERIALIZED VIEW U1.M_HARD_COL_ACTIVITYRESULT
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ACTIVITYRESULT"."ID" "ID","ACTIVITYRESULT"."CODE" "CODE","ACTIVITYRESULT"."ORD" "ORD","ACTIVITYRESULT"."VALUE" "VALUE" FROM "DEBTCOLL"."ACTIVITYRESULT"@"HCOL" "ACTIVITYRESULT";
grant select on U1.M_HARD_COL_ACTIVITYRESULT to LOADDB;


