AS
SELECT "JURISDICTIONCOUNTRY"."ID" "ID","JURISDICTIONCOUNTRY"."CODE" "CODE","JURISDICTIONCOUNTRY"."ORD" "ORD","JURISDICTIONCOUNTRY"."VALUE" "VALUE" FROM "DEBTCOLL"."JURISDICTIONCOUNTRY"@"HCOL" "JURISDICTIONCOUNTRY";
grant select on U1.M_HARD_COL_JURISDICTIONCOUNTRY to LOADDB;


