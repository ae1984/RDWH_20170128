CREATE MATERIALIZED VIEW U1.M_HARD_COL_CREDITPROGRAMM
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CREDITPROGRAMM"."ID" "ID","CREDITPROGRAMM"."CODE" "CODE","CREDITPROGRAMM"."ORD" "ORD","CREDITPROGRAMM"."VALUE" "VALUE" FROM "DEBTCOLL"."CREDITPROGRAMM"@"HCOL" "CREDITPROGRAMM";
grant select on U1.M_HARD_COL_CREDITPROGRAMM to LOADDB;


