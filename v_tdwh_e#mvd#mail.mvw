CREATE MATERIALIZED VIEW U1.V_TDWH_E#MVD#MAIL
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."SENT_DATE" "SENT_DATE","T"."FILE_CLOB" "FILE_CLOB","T"."RECEIVE_TS" "RECEIVE_TS","T"."FILE_DATE" "FILE_DATE","T"."SUBJECT" "SUBJECT","T"."FILE_NAME" "FILE_NAME","T"."D#MVD#MAIL_STATUS" "D#MVD#MAIL_STATUS" FROM "VIN"."E#MVD#MAIL"@"TDWHTEST" "T";
grant select on U1.V_TDWH_E#MVD#MAIL to LOADDB;
grant select on U1.V_TDWH_E#MVD#MAIL to LOADER;


