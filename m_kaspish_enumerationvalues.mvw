create materialized view U1.M_KASPISH_ENUMERATIONVALUES
build deferred
refresh force on demand
as
select
"HJMPTS","TYPEPKSTRING","OWNERPKSTRING","MODIFIEDTS","CREATEDTS","PK","CODE","P_ICON","SEQUENCENUMBER","P_EXTENSIONNAME","ACLTS","PROPTS","EDITABLE","CODELOWERCASE"
from kaspi.ENUMERATIONVALUES@ks_std
where rownum >=0;
grant select on U1.M_KASPISH_ENUMERATIONVALUES to LOADDB;


