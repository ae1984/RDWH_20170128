CREATE MATERIALIZED VIEW U1.M_HARD_COL_CERTIFICATE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CERTIFICATE"."ID" "ID","CERTIFICATE"."ACTUALDATEFROM" "ACTUALDATEFROM","CERTIFICATE"."ACTUALDATETO" "ACTUALDATETO","CERTIFICATE"."ISSUEDATE" "ISSUEDATE","CERTIFICATE"."ISSUEPLACE" "ISSUEPLACE","CERTIFICATE"."ISSUEWHO" "ISSUEWHO","CERTIFICATE"."NUMBER" "NUMBER","CERTIFICATE"."SERIAL" "SERIAL","CERTIFICATE"."TYPE" "TYPE","CERTIFICATE"."OWNERCLIENT_ID" "OWNERCLIENT_ID" FROM "DEBTCOLL"."CERTIFICATE"@"HCOL" "CERTIFICATE";
grant select on U1.M_HARD_COL_CERTIFICATE to LOADDB;


