CREATE MATERIALIZED VIEW U1.V_MO_D_PROCESS_PAR
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."IS_NULLABLE" "IS_NULLABLE","T"."BATCH_ID" "BATCH_ID","T"."IS_OUT" "IS_OUT","T"."STATUS" "STATUS","T"."DESCR" "DESCR","T"."VERSION_NUM" "VERSION_NUM","T"."STATUS_NEW" "STATUS_NEW","T"."D_PAR_CODE" "D_PAR_CODE","T"."D_PROCESS_CODE" "D_PROCESS_CODE" FROM "D_PROCESS_PAR"@"MO1_PROD" "T";
grant select on U1.V_MO_D_PROCESS_PAR to LOADDB;
grant select on U1.V_MO_D_PROCESS_PAR to LOADER;


