CREATE MATERIALIZED VIEW U1.V_RFO_Z#CM_USER
REFRESH COMPLETE ON DEMAND
AS
SELECT "Z#CM_USER"."ID" "ID","Z#CM_USER"."COLLECTION_ID" "COLLECTION_ID","Z#CM_USER"."C_USER" "C_USER","Z#CM_USER"."C_BEG_TIME" "C_BEG_TIME","Z#CM_USER"."C_END_TIME" "C_END_TIME","Z#CM_USER"."C_ROLES" "C_ROLES","Z#CM_USER"."SN" "SN","Z#CM_USER"."SU" "SU" FROM "IBS"."Z#CM_USER"@"RFO_SNAP" "Z#CM_USER";
grant select on U1.V_RFO_Z#CM_USER to LOADDB;
grant select on U1.V_RFO_Z#CM_USER to LOADER;


