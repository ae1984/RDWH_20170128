create table U1.SB_LIST_TMP
(
  sb_fio        VARCHAR2(200),
  sb_birth_date DATE,
  sb_rnn        VARCHAR2(12),
  sb_iin        VARCHAR2(12)
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.SB_LIST_TMP to LOADDB;
grant select on U1.SB_LIST_TMP to LOADER;

