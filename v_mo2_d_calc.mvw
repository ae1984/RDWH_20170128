CREATE MATERIALIZED VIEW U1.V_MO2_D_CALC
REFRESH COMPLETE ON DEMAND
AS
SELECT "D_CALC"."ID" "ID","D_CALC"."CODE" "CODE","D_CALC"."NAME" "NAME","D_CALC"."SCRIPT" "SCRIPT","D_CALC"."STATUS" "STATUS","D_CALC"."BATCH_ID" "BATCH_ID","D_CALC"."DESCR" "DESCR","D_CALC"."VERSION_NUM" "VERSION_NUM","D_CALC"."RUN_TYPE" "RUN_TYPE","D_CALC"."STATUS_NEW" "STATUS_NEW","D_CALC"."D_CALC_TYPE_CODE" "D_CALC_TYPE_CODE","D_CALC"."D_CALC_GROUP_CODE" "D_CALC_GROUP_CODE" FROM "D_CALC"@"MO2_PROD" "D_CALC";
grant select on U1.V_MO2_D_CALC to LOADDB;
grant select on U1.V_MO2_D_CALC to LOADER;


