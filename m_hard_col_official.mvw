CREATE MATERIALIZED VIEW U1.M_HARD_COL_OFFICIAL
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "OFFICIAL"."ID" "ID","OFFICIAL"."CREATEDDATE" "CREATEDDATE","OFFICIAL"."MODIFIEDDATE" "MODIFIEDDATE","OFFICIAL"."ADDRESSLIVE" "ADDRESSLIVE","OFFICIAL"."ADDRESSREGISTRATION" "ADDRESSREGISTRATION","OFFICIAL"."BIRTHDATE" "BIRTHDATE","OFFICIAL"."DATEWORKFROM" "DATEWORKFROM","OFFICIAL"."DATEWORKTO" "DATEWORKTO","OFFICIAL"."DOCISSUEDATE" "DOCISSUEDATE","OFFICIAL"."DOCISSUEWHOM" "DOCISSUEWHOM","OFFICIAL"."DOCNUM" "DOCNUM","OFFICIAL"."DOCSER" "DOCSER","OFFICIAL"."DOCTYPE" "DOCTYPE","OFFICIAL"."NAME" "NAME","OFFICIAL"."PATRONYMIC" "PATRONYMIC","OFFICIAL"."POSITION" "POSITION","OFFICIAL"."RNN" "RNN","OFFICIAL"."SIC" "SIC","OFFICIAL"."SURNAME" "SURNAME","OFFICIAL"."AUTHOR_ID" "AUTHOR_ID","OFFICIAL"."MODIFIEDBY_ID" "MODIFIEDBY_ID","OFFICIAL"."IIN" "IIN" FROM "DEBTCOLL"."OFFICIAL"@"HCOL" "OFFICIAL";
grant select on U1.M_HARD_COL_OFFICIAL to LOADDB;


