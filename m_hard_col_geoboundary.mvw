CREATE MATERIALIZED VIEW U1.M_HARD_COL_GEOBOUNDARY
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "GEOBOUNDARY"."ID" "ID","GEOBOUNDARY"."CODE" "CODE","GEOBOUNDARY"."ORD" "ORD","GEOBOUNDARY"."VALUE" "VALUE","GEOBOUNDARY"."TYPE" "TYPE","GEOBOUNDARY"."PARENT_ID" "PARENT_ID" FROM "DEBTCOLL"."GEOBOUNDARY"@"HCOL" "GEOBOUNDARY";
grant select on U1.M_HARD_COL_GEOBOUNDARY to LOADDB;


