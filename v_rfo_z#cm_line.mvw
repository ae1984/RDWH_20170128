CREATE MATERIALIZED VIEW U1.V_RFO_Z#CM_LINE
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "Z#CM_LINE"."ID" "ID","Z#CM_LINE"."COLLECTION_ID" "COLLECTION_ID","Z#CM_LINE"."C_NAME" "C_NAME","Z#CM_LINE"."C_FROM_P" "C_FROM_P","Z#CM_LINE"."C_TO_P" "C_TO_P","Z#CM_LINE"."C_CONDITION" "C_CONDITION","Z#CM_LINE"."C_ACTION" "C_ACTION","Z#CM_LINE"."C_CODE" "C_CODE","Z#CM_LINE"."SN" "SN","Z#CM_LINE"."SU" "SU" FROM "IBS"."Z#CM_LINE"@"RFO_SNAP" "Z#CM_LINE";
grant select on U1.V_RFO_Z#CM_LINE to LOADDB;
grant select on U1.V_RFO_Z#CM_LINE to LOADER;


