CREATE MATERIALIZED VIEW U1.M_HARD_COL_CLIENT_ADDRESS_AUD
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CLIENT_ADDRESS_AUD"."REV" "REV","CLIENT_ADDRESS_AUD"."CLIENT_ID" "CLIENT_ID","CLIENT_ADDRESS_AUD"."ADDRESS_ID" "ADDRESS_ID","CLIENT_ADDRESS_AUD"."REVTYPE" "REVTYPE" FROM "DEBTCOLL"."CLIENT_ADDRESS_AUD"@"HCOL" "CLIENT_ADDRESS_AUD";
grant select on U1.M_HARD_COL_CLIENT_ADDRESS_AUD to LOADDB;


