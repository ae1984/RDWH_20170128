CREATE MATERIALIZED VIEW U1.V_RFO_Z#CM_STEP
REFRESH COMPLETE ON DEMAND
AS
SELECT "Z#CM_STEP"."ID" "ID","Z#CM_STEP"."COLLECTION_ID" "COLLECTION_ID","Z#CM_STEP"."C_N" "C_N","Z#CM_STEP"."C_RUN_TYPE" "C_RUN_TYPE","Z#CM_STEP"."C_NAME" "C_NAME","Z#CM_STEP"."C_CONDITION" "C_CONDITION","Z#CM_STEP"."CLASS_ID" "CLASS_ID","Z#CM_STEP"."SN" "SN","Z#CM_STEP"."SU" "SU" FROM "IBS"."Z#CM_STEP"@"RFO_SNAP" "Z#CM_STEP";
grant select on U1.V_RFO_Z#CM_STEP to LOADDB;
grant select on U1.V_RFO_Z#CM_STEP to LOADER;


