AS
SELECT "CLIENTCONTACT"."ID" "ID","CLIENTCONTACT"."CREATEDDATE" "CREATEDDATE","CLIENTCONTACT"."MODIFIEDDATE" "MODIFIEDDATE","CLIENTCONTACT"."EDITABLE" "EDITABLE","CLIENTCONTACT"."NAME" "NAME","CLIENTCONTACT"."PATRONYMIC" "PATRONYMIC","CLIENTCONTACT"."PHONENUMBER" "PHONENUMBER","CLIENTCONTACT"."SURNAME" "SURNAME","CLIENTCONTACT"."AUTHOR_ID" "AUTHOR_ID","CLIENTCONTACT"."MODIFIEDBY_ID" "MODIFIEDBY_ID","CLIENTCONTACT"."TERM_ID" "TERM_ID","CLIENTCONTACT"."CLEARPHONENUMBER" "CLEARPHONENUMBER","CLIENTCONTACT"."EXCLUDED" "EXCLUDED","CLIENTCONTACT"."EXCLUDEDTO" "EXCLUDEDTO","CLIENTCONTACT"."ISPAYINDIVIDUAL" "ISPAYINDIVIDUAL" FROM "DEBTCOLL"."CLIENTCONTACT"@"HCOL" "CLIENTCONTACT";
grant select on U1.M_HARD_COL_CLIENTCONTACT to LOADDB;


