CREATE MATERIALIZED VIEW U1.V_MO_D_CALC
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."CODE" "CODE","T"."NAME" "NAME","T"."SCRIPT" "SCRIPT","T"."STATUS" "STATUS","T"."BATCH_ID" "BATCH_ID","T"."DESCR" "DESCR","T"."VERSION_NUM" "VERSION_NUM","T"."RUN_TYPE" "RUN_TYPE","T"."STATUS_NEW" "STATUS_NEW","T"."D_CALC_TYPE_CODE" "D_CALC_TYPE_CODE","T"."D_CALC_GROUP_CODE" "D_CALC_GROUP_CODE" FROM "D_CALC"@"MO1_PROD" "T";
grant select on U1.V_MO_D_CALC to LOADDB;
grant select on U1.V_MO_D_CALC to LOADER;


