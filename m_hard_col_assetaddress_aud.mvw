REFRESH FORCE ON DEMAND
AS
SELECT "ASSETADDRESS_AUD"."ID" "ID","ASSETADDRESS_AUD"."REV" "REV","ASSETADDRESS_AUD"."REVTYPE" "REVTYPE","ASSETADDRESS_AUD"."FLAT" "FLAT","ASSETADDRESS_AUD"."HOUSE" "HOUSE","ASSETADDRESS_AUD"."POSTALCODE" "POSTALCODE","ASSETADDRESS_AUD"."STREET" "STREET","ASSETADDRESS_AUD"."SETTLEMENT_ID" "SETTLEMENT_ID","ASSETADDRESS_AUD"."STATE_ID" "STATE_ID" FROM "DEBTCOLL"."ASSETADDRESS_AUD"@"HCOL" "ASSETADDRESS_AUD";
grant select on U1.M_HARD_COL_ASSETADDRESS_AUD to LOADDB;


