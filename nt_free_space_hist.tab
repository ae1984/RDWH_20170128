create table U1.NT_FREE_SPACE_HIST
(
  sdt             DATE,
  tablespace_name VARCHAR2(30),
  mbytes_free     NUMBER
)
tablespace USERS_F
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.NT_FREE_SPACE_HIST to LOADDB;

