CREATE MATERIALIZED VIEW U1.V_MO_D_PROCESS
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."NAME" "NAME","T"."CODE" "CODE","T"."STATUS" "STATUS","T"."BATCH_ID" "BATCH_ID","T"."DESCR" "DESCR","T"."VERSION_NUM" "VERSION_NUM","T"."STATUS_NEW" "STATUS_NEW","T"."D_PROCESS_TYPE_CODE" "D_PROCESS_TYPE_CODE","T"."D_SYSTEM_CODE" "D_SYSTEM_CODE","T"."IS_OUT_PAR_SEND" "IS_OUT_PAR_SEND" FROM "D_PROCESS"@"MO1_PROD" "T";
grant select on U1.V_MO_D_PROCESS to LOADDB;
grant select on U1.V_MO_D_PROCESS to LOADER;


