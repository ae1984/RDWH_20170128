CREATE MATERIALIZED VIEW U1.M_HARD_COL_INCOMINGREST_AUD
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "INCOMINGREST_AUD"."ID" "ID","INCOMINGREST_AUD"."REV" "REV","INCOMINGREST_AUD"."REVTYPE" "REVTYPE","INCOMINGREST_AUD"."ACCRUEDREWARD" "ACCRUEDREWARD","INCOMINGREST_AUD"."OVERDUEDEBT" "OVERDUEDEBT","INCOMINGREST_AUD"."OVERDUEREWARD" "OVERDUEREWARD","INCOMINGREST_AUD"."URGENTDEBT" "URGENTDEBT" FROM "DEBTCOLL"."INCOMINGREST_AUD"@"HCOL" "INCOMINGREST_AUD";
grant select on U1.M_HARD_COL_INCOMINGREST_AUD to LOADDB;


