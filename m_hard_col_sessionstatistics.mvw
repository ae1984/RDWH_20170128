CREATE MATERIALIZED VIEW U1.M_HARD_COL_SESSIONSTATISTICS
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "SESSIONSTATISTICS"."ID" "ID","SESSIONSTATISTICS"."COUNTACTIVITIES" "COUNTACTIVITIES","SESSIONSTATISTICS"."COUNTDOCUMENTS" "COUNTDOCUMENTS","SESSIONSTATISTICS"."COUNTINDEPENDENTASSESSMENTS" "COUNTINDEPENDENTASSESSMENTS","SESSIONSTATISTICS"."COUNTINTERNALASSESSMENTS" "COUNTINTERNALASSESSMENTS","SESSIONSTATISTICS"."COUNTMONITORINGASSESSMENTS" "COUNTMONITORINGASSESSMENTS","SESSIONSTATISTICS"."COUNTPHOTOS" "COUNTPHOTOS","SESSIONSTATISTICS"."SESSION_ID" "SESSION_ID" FROM "DEBTCOLL"."SESSIONSTATISTICS"@"HCOL" "SESSIONSTATISTICS";
grant select on U1.M_HARD_COL_SESSIONSTATISTICS to LOADDB;


