CREATE MATERIALIZED VIEW U1.M_HARD_COL_APPLICATION_PROPERT
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "APPLICATION_PROPERTIES"."CONFIG_KEY" "CONFIG_KEY","APPLICATION_PROPERTIES"."CONFIG_VALUE" "CONFIG_VALUE" FROM "DEBTCOLL"."APPLICATION_PROPERTIES"@"HCOL" "APPLICATION_PROPERTIES";
grant select on U1.M_HARD_COL_APPLICATION_PROPERT to LOADDB;


