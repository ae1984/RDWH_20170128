create table U1.NT_SQL_OBJECTNAME
(
  sql_id           VARCHAR2(13),
  plan_object_name VARCHAR2(30),
  owner            VARCHAR2(128) not null,
  cnt_obj_by_sql   NUMBER,
  object_name      VARCHAR2(30)
)
tablespace USERS
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.NT_SQL_OBJECTNAME to LOADDB;

