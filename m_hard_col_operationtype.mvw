CREATE MATERIALIZED VIEW U1.M_HARD_COL_OPERATIONTYPE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "OPERATIONTYPE"."ID" "ID","OPERATIONTYPE"."CODE" "CODE","OPERATIONTYPE"."ORD" "ORD","OPERATIONTYPE"."VALUE" "VALUE" FROM "DEBTCOLL"."OPERATIONTYPE"@"HCOL" "OPERATIONTYPE";
grant select on U1.M_HARD_COL_OPERATIONTYPE to LOADDB;


