CREATE MATERIALIZED VIEW U1.M_OUT_DWH_VERIFY_GROUP
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "H_VERIFY_GROUP"."ID" "ID","H_VERIFY_GROUP"."CODE" "CODE","H_VERIFY_GROUP"."NAME" "NAME" FROM "VERIFICATOR"."H_VERIFY_GROUP"@"VERIFAIS" "H_VERIFY_GROUP";
grant select on U1.M_OUT_DWH_VERIFY_GROUP to LOADDB;
grant select on U1.M_OUT_DWH_VERIFY_GROUP to LOADER;


