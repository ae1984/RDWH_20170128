create table U1.LOG_USER_ACTIONS
(
  action     VARCHAR2(100),
  object     VARCHAR2(150),
  start_time DATE,
  users      VARCHAR2(100)
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.LOG_USER_ACTIONS to LOADDB;
grant select on U1.LOG_USER_ACTIONS to LOADER;

