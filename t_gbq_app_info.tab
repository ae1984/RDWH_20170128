create table U1.T_GBQ_APP_INFO
(
  id           NUMBER,
  hit_id       VARCHAR2(50),
  app_name     VARCHAR2(100),
  app_version  VARCHAR2(100),
  app_id       VARCHAR2(100),
  installer_id VARCHAR2(100),
  idate        TIMESTAMP(6)
)
tablespace USERS_F
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.T_GBQ_APP_INFO to LOADDB;

