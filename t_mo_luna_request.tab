create table U1.T_MO_LUNA_REQUEST
(
  id            NUMBER not null,
  request_id    NUMBER,
  luna_match_id NUMBER,
  similarity    NUMBER,
  luna_status   VARCHAR2(64),
  luna_err_msg  VARCHAR2(2048),
  date_insert   DATE
)
tablespace USERS_F
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 8M
    minextents 1
    maxextents unlimited
  )
