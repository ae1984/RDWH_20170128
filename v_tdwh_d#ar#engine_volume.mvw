CREATE MATERIALIZED VIEW U1.V_TDWH_D#AR#ENGINE_VOLUME
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."CODE" "CODE","T"."RUS" "RUS","T"."ENG" "ENG","T"."KAZ" "KAZ","T"."RUS_DESC" "RUS_DESC","T"."ENG_DESC" "ENG_DESC","T"."KAZ_DESC" "KAZ_DESC","T"."NUM#" "NUM#" FROM "VIN"."D#AR#ENGINE_VOLUME"@"TDWHTEST" "T";
grant select on U1.V_TDWH_D#AR#ENGINE_VOLUME to LOADDB;
grant select on U1.V_TDWH_D#AR#ENGINE_VOLUME to LOADER;


