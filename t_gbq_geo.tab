create table U1.T_GBQ_GEO
(
  id     NUMBER,
  hit_id VARCHAR2(50),
  geo_id VARCHAR2(100),
  idate  TIMESTAMP(6)
)
tablespace USERS_F
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
comment on column U1.T_GBQ_GEO.id
  is 'Идентификатор';
comment on column U1.T_GBQ_GEO.hit_id
  is 'ID посещения страницы';
grant select on U1.T_GBQ_GEO to LOADDB;

