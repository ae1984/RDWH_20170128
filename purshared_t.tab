create table U1.PURSHARED_T
(
  purchase NUMBER(1),
  income   NUMBER(7,2),
  age      NUMBER(10),
  zip      NUMBER(10),
  id       NUMBER
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.PURSHARED_T to LOADDB;
grant select on U1.PURSHARED_T to LOADER;

