grant select on U1.V_RFO_Z#PROPERTY_GRP to ITS_USER;
grant select on U1.V_RFO_Z#PROPERTY_GRP to LOADDB;
grant select on U1.V_RFO_Z#PROPERTY_GRP to LOADER;


