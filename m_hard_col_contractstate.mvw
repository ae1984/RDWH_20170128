CREATE MATERIALIZED VIEW U1.M_HARD_COL_CONTRACTSTATE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CONTRACTSTATE"."ID" "ID","CONTRACTSTATE"."CODE" "CODE","CONTRACTSTATE"."ORD" "ORD","CONTRACTSTATE"."VALUE" "VALUE" FROM "DEBTCOLL"."CONTRACTSTATE"@"HCOL" "CONTRACTSTATE";
grant select on U1.M_HARD_COL_CONTRACTSTATE to LOADDB;


