create table U1.TMP_PURSHARED_T
(
  purchase VARCHAR2(10),
  income   VARCHAR2(100),
  age      VARCHAR2(10),
  zip      VARCHAR2(10)
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.TMP_PURSHARED_T to LOADDB;
grant select on U1.TMP_PURSHARED_T to LOADER;

