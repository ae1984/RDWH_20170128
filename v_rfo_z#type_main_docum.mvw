CREATE MATERIALIZED VIEW U1.V_RFO_Z#TYPE_MAIN_DOCUM
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "Z#TYPE_MAIN_DOCUM"."ID" "ID","Z#TYPE_MAIN_DOCUM"."C_CODE" "C_CODE","Z#TYPE_MAIN_DOCUM"."C_NAME" "C_NAME","Z#TYPE_MAIN_DOCUM"."SN" "SN","Z#TYPE_MAIN_DOCUM"."SU" "SU" FROM "IBS"."Z#TYPE_MAIN_DOCUM"@"RFO_SNAP" "Z#TYPE_MAIN_DOCUM";
grant select on U1.V_RFO_Z#TYPE_MAIN_DOCUM to LOADDB;
grant select on U1.V_RFO_Z#TYPE_MAIN_DOCUM to LOADER;


