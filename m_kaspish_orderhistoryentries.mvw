create materialized view U1.M_KASPISH_ORDERHISTORYENTRIES
build deferred
refresh force on demand
as
select
   "HJMPTS","TYPEPKSTRING","OWNERPKSTRING","MODIFIEDTS","CREATEDTS","PK","P_ORDERPOS","P_PREVIOUSORDERVERSION","P_EMPLOYEE","P_DESCRIPTION","P_ORDER","P_TIMESTAMP","P_ORIGINATORNAME","P_ORIGINATOR","P_MESSAGE","STATUSPK","ACLTS","PROPTS"
from kaspi.ORDERHISTORYENTRIES@ks_std
where rownum >=0;
grant select on U1.M_KASPISH_ORDERHISTORYENTRIES to LOADDB;


