AS
SELECT "JUDICIALWORKER"."ID" "ID","JUDICIALWORKER"."CREATEDDATE" "CREATEDDATE","JUDICIALWORKER"."NAME" "NAME","JUDICIALWORKER"."PATRONYMIC" "PATRONYMIC","JUDICIALWORKER"."PHONES" "PHONES","JUDICIALWORKER"."REGION" "REGION","JUDICIALWORKER"."ROLE" "ROLE","JUDICIALWORKER"."SURNAME" "SURNAME","JUDICIALWORKER"."CREATEDBY_ID" "CREATEDBY_ID","JUDICIALWORKER"."LOCATION_ID" "LOCATION_ID" FROM "DEBTCOLL"."JUDICIALWORKER"@"HCOL" "JUDICIALWORKER";
grant select on U1.M_HARD_COL_JUDICIALWORKER to LOADDB;


