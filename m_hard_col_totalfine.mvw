CREATE MATERIALIZED VIEW U1.M_HARD_COL_TOTALFINE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "TOTALFINE"."ID" "ID","TOTALFINE"."DATE" "DATE","TOTALFINE"."SUM_AMOUNT" "SUM_AMOUNT","TOTALFINE"."SUM_CURRENCY" "SUM_CURRENCY","TOTALFINE"."CONTRACT_ID" "CONTRACT_ID" FROM "DEBTCOLL"."TOTALFINE"@"HCOL" "TOTALFINE";
grant select on U1.M_HARD_COL_TOTALFINE to LOADDB;


