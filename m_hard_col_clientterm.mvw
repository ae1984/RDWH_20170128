CREATE MATERIALIZED VIEW U1.M_HARD_COL_CLIENTTERM
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CLIENTTERM"."ID" "ID","CLIENTTERM"."CODE" "CODE","CLIENTTERM"."ORD" "ORD","CLIENTTERM"."VALUE" "VALUE" FROM "DEBTCOLL"."CLIENTTERM"@"HCOL" "CLIENTTERM";
grant select on U1.M_HARD_COL_CLIENTTERM to LOADDB;


