CREATE MATERIALIZED VIEW U1.M_HARD_COL_ASSET_AUD
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ASSET_AUD"."ID" "ID","ASSET_AUD"."REV" "REV","ASSET_AUD"."REVTYPE" "REVTYPE","ASSET_AUD"."DESCRIPTION" "DESCRIPTION","ASSET_AUD"."CATEGORY_ID" "CATEGORY_ID","ASSET_AUD"."REGISTRATIONADDRESS_ID" "REGISTRATIONADDRESS_ID","ASSET_AUD"."ZALOGDESC" "ZALOGDESC" FROM "DEBTCOLL"."ASSET_AUD"@"HCOL" "ASSET_AUD";
grant select on U1.M_HARD_COL_ASSET_AUD to LOADDB;


