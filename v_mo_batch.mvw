CREATE MATERIALIZED VIEW U1.V_MO_BATCH
REFRESH COMPLETE ON DEMAND
AS
SELECT "BATCH"."ID" "ID","BATCH"."DATE_CREATE" "DATE_CREATE","BATCH"."DATE_APPLY" "DATE_APPLY","BATCH"."DESCR" "DESCR","BATCH"."VERSION_NUM" "VERSION_NUM" FROM "BATCH"@"MO1_PROD" "BATCH";
grant select on U1.V_MO_BATCH to LOADDB;
grant select on U1.V_MO_BATCH to LOADER;


