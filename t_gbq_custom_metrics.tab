create table U1.T_GBQ_CUSTOM_METRICS
(
  id           NUMBER,
  hit_id       VARCHAR2(50),
  metric_index NUMBER(6),
  metric_value VARCHAR2(100),
  idate        TIMESTAMP(6)
)
tablespace USERS_F
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 8M
    minextents 1
    maxextents unlimited
  )
compress
nologging;
grant select on U1.T_GBQ_CUSTOM_METRICS to LOADDB;

