CREATE MATERIALIZED VIEW U1.V_MO_D_MSG_PAR
REFRESH FORCE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."CODE" "CODE","T"."MSG_CODE" "MSG_CODE","T"."MSG_PAR_VALUE" "MSG_PAR_VALUE","T"."MSG_PAR_ORDER" "MSG_PAR_ORDER","T"."DESCR" "DESCR","T"."BATCH_ID" "BATCH_ID","T"."STATUS" "STATUS","T"."VERSION_NUM" "VERSION_NUM","T"."STATUS_NEW" "STATUS_NEW" FROM "U1"."D_MSG_PAR"@"MO1_PROD" "T";
grant select on U1.V_MO_D_MSG_PAR to LOADDB;
grant select on U1.V_MO_D_MSG_PAR to LOADER;


