CREATE MATERIALIZED VIEW U1.V_RFO_Z#TRANSPORT
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "Z#TRANSPORT"."ID" "ID","Z#TRANSPORT"."C_PASSPORT" "C_PASSPORT","Z#TRANSPORT"."C_VIN" "C_VIN","Z#TRANSPORT"."C_MODEL" "C_MODEL","Z#TRANSPORT"."C_CATEG" "C_CATEG","Z#TRANSPORT"."C_YEARS" "C_YEARS","Z#TRANSPORT"."C_REG_NUM" "C_REG_NUM","Z#TRANSPORT"."C_PRODUCTION_REF" "C_PRODUCTION_REF","Z#TRANSPORT"."C_BRAND_REF" "C_BRAND_REF","Z#TRANSPORT"."C_MODEL_REF" "C_MODEL_REF","Z#TRANSPORT"."CLASS_ID" "CLASS_ID","Z#TRANSPORT"."SN" "SN","Z#TRANSPORT"."SU" "SU" FROM "IBS"."Z#TRANSPORT"@"RFO_SNAP" "Z#TRANSPORT";
grant select on U1.V_RFO_Z#TRANSPORT to LOADDB;
grant select on U1.V_RFO_Z#TRANSPORT to LOADER;


