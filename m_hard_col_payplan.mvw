CREATE MATERIALIZED VIEW U1.M_HARD_COL_PAYPLAN
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "PAYPLAN"."ID" "ID","PAYPLAN"."AMOUNT_AMOUNT" "AMOUNT_AMOUNT","PAYPLAN"."AMOUNT_CURRENCY" "AMOUNT_CURRENCY","PAYPLAN"."FILIAL" "FILIAL","PAYPLAN"."ISEXECUTED" "ISEXECUTED","PAYPLAN"."OPDATE" "OPDATE","PAYPLAN"."OPNAME" "OPNAME","PAYPLAN"."STATUS" "STATUS","PAYPLAN"."SUMMADEBT" "SUMMADEBT","PAYPLAN"."SUMMAEXEC" "SUMMAEXEC","PAYPLAN"."OPERATIONTYPE_ID" "OPERATIONTYPE_ID","PAYPLAN"."CONTRACT_ID" "CONTRACT_ID" FROM "DEBTCOLL"."PAYPLAN"@"HCOL" "PAYPLAN";
grant select on U1.M_HARD_COL_PAYPLAN to LOADDB;


