CREATE MATERIALIZED VIEW U1.M_HARD_COL_ADDRESS_AUD
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ADDRESS_AUD"."ID" "ID","ADDRESS_AUD"."REV" "REV","ADDRESS_AUD"."REVTYPE" "REVTYPE","ADDRESS_AUD"."ADDRESSSTR" "ADDRESSSTR","ADDRESS_AUD"."POSTCODE" "POSTCODE","ADDRESS_AUD"."TYPE" "TYPE" FROM "DEBTCOLL"."ADDRESS_AUD"@"HCOL" "ADDRESS_AUD";
grant select on U1.M_HARD_COL_ADDRESS_AUD to LOADDB;


