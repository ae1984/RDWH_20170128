CREATE MATERIALIZED VIEW U1.V_RFO_Z#ADDR_TYPE
REFRESH COMPLETE ON DEMAND
AS
SELECT "Z#ADDR_TYPE"."ID" "ID","Z#ADDR_TYPE"."C_CODE" "C_CODE","Z#ADDR_TYPE"."C_NAME" "C_NAME","Z#ADDR_TYPE"."SN" "SN","Z#ADDR_TYPE"."SU" "SU" FROM "IBS"."Z#ADDR_TYPE"@"RFO_SNAP" "Z#ADDR_TYPE";
grant select on U1.V_RFO_Z#ADDR_TYPE to LOADDB;
grant select on U1.V_RFO_Z#ADDR_TYPE to LOADER;


