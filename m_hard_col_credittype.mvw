CREATE MATERIALIZED VIEW U1.M_HARD_COL_CREDITTYPE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CREDITTYPE"."ID" "ID","CREDITTYPE"."CODE" "CODE","CREDITTYPE"."ORD" "ORD","CREDITTYPE"."VALUE" "VALUE" FROM "DEBTCOLL"."CREDITTYPE"@"HCOL" "CREDITTYPE";
grant select on U1.M_HARD_COL_CREDITTYPE to LOADDB;


