  sql_id   VARCHAR2(13),
  module   VARCHAR2(64),
  machine  VARCHAR2(64),
  username VARCHAR2(30),
  osuser   VARCHAR2(30),
  cnt      NUMBER
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
