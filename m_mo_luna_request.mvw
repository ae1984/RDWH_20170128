CREATE MATERIALIZED VIEW U1.M_MO_LUNA_REQUEST
REFRESH FORCE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."REQUEST_ID" "REQUEST_ID","T"."LUNA_MATCH_ID" "LUNA_MATCH_ID","T"."SIMILARITY" "SIMILARITY","T"."LUNA_STATUS" "LUNA_STATUS","T"."LUNA_ERR_MSG" "LUNA_ERR_MSG","T"."DATE_INSERT" "DATE_INSERT" FROM "U1"."T_LUNA_REQUEST"@"MO1_PROD" "T";
grant select on U1.M_MO_LUNA_REQUEST to LOADDB;
grant select on U1.M_MO_LUNA_REQUEST to LOADER;


