CREATE MATERIALIZED VIEW U1.V_MO_D_PROCESS_CALC
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."CALC_ORDER" "CALC_ORDER","T"."STATUS" "STATUS","T"."D_PROCESS_CODE" "D_PROCESS_CODE","T"."D_CALC_CODE" "D_CALC_CODE","T"."BATCH_ID" "BATCH_ID","T"."DESCR" "DESCR","T"."VERSION_NUM" "VERSION_NUM","T"."STATUS_NEW" "STATUS_NEW","T"."CODE" "CODE","T"."D_SUB_PROCESS_CODE" "D_SUB_PROCESS_CODE" FROM "D_PROCESS_CALC"@"MO1_PROD" "T";
grant select on U1.V_MO_D_PROCESS_CALC to LOADDB;
grant select on U1.V_MO_D_PROCESS_CALC to LOADER;


