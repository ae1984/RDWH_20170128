CREATE MATERIALIZED VIEW U1.M_HARD_COL_CREDITSTATUS
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "CREDITSTATUS"."ID" "ID","CREDITSTATUS"."CODE" "CODE","CREDITSTATUS"."ORD" "ORD","CREDITSTATUS"."VALUE" "VALUE" FROM "DEBTCOLL"."CREDITSTATUS"@"HCOL" "CREDITSTATUS";
grant select on U1.M_HARD_COL_CREDITSTATUS to LOADDB;


