CREATE MATERIALIZED VIEW U1.V_TDWH_E#VIN
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."IS_VALID" "IS_VALID","T"."CREATE_DATE" "CREATE_DATE","T"."VIN" "VIN" FROM "VIN"."E#VIN"@"TDWHTEST" "T";
grant select on U1.V_TDWH_E#VIN to LOADDB;
grant select on U1.V_TDWH_E#VIN to LOADER;


