AS
SELECT "Z#CM_CONDITION"."ID" "ID","Z#CM_CONDITION"."C_TEXT" "C_TEXT","Z#CM_CONDITION"."C_CODE" "C_CODE","Z#CM_CONDITION"."C_NAME" "C_NAME","Z#CM_CONDITION"."C_CONDITION_GROUP" "C_CONDITION_GROUP","Z#CM_CONDITION"."SN" "SN","Z#CM_CONDITION"."SU" "SU" FROM "IBS"."Z#CM_CONDITION"@"RFO_SNAP" "Z#CM_CONDITION";
grant select on U1.V_RFO_Z#CM_CONDITION to LOADDB;
grant select on U1.V_RFO_Z#CM_CONDITION to LOADER;


