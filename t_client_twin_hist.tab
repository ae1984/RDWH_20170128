  rfo_client_id      NUMBER not null,
  client_iin         VARCHAR2(36),
  twin_rfo_client_id NUMBER,
  twin_client_iin    VARCHAR2(36),
  twin_cnt           NUMBER
)
tablespace USERS_ARC
  pctfree 0
  initrans 1
  maxtrans 255
  storage
  (
