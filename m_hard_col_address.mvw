CREATE MATERIALIZED VIEW U1.M_HARD_COL_ADDRESS
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ADDRESS"."ID" "ID","ADDRESS"."ADDRESSSTR" "ADDRESSSTR","ADDRESS"."POSTCODE" "POSTCODE","ADDRESS"."TYPE" "TYPE" FROM "DEBTCOLL"."ADDRESS"@"HCOL" "ADDRESS";
grant select on U1.M_HARD_COL_ADDRESS to LOADDB;


