CREATE MATERIALIZED VIEW U1.M_HARD_COL_LEGALSTAGE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "LEGALSTAGE"."ID" "ID","LEGALSTAGE"."BAILIFF_ID" "BAILIFF_ID","LEGALSTAGE"."JUDGE_ID" "JUDGE_ID" FROM "DEBTCOLL"."LEGALSTAGE"@"HCOL" "LEGALSTAGE";
grant select on U1.M_HARD_COL_LEGALSTAGE to LOADDB;


