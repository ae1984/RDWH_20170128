CREATE MATERIALIZED VIEW U1.M_HARD_COL_ACTIVITYTYPE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ACTIVITYTYPE"."ID" "ID","ACTIVITYTYPE"."CODE" "CODE","ACTIVITYTYPE"."ORD" "ORD","ACTIVITYTYPE"."VALUE" "VALUE","ACTIVITYTYPE"."CONTRACTKIND" "CONTRACTKIND" FROM "DEBTCOLL"."ACTIVITYTYPE"@"HCOL" "ACTIVITYTYPE";
grant select on U1.M_HARD_COL_ACTIVITYTYPE to LOADDB;


