CREATE MATERIALIZED VIEW U1.V_RFO_Z#PERIOD
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "Z#PERIOD"."ID" "ID","Z#PERIOD"."C_COND" "C_COND","Z#PERIOD"."C_MIN_PERIOD#QUANT" "C_MIN_PERIOD#QUANT","Z#PERIOD"."C_MIN_PERIOD#UNIT" "C_MIN_PERIOD#UNIT","Z#PERIOD"."C_MAX_PERIOD#QUANT" "C_MAX_PERIOD#QUANT","Z#PERIOD"."C_MAX_PERIOD#UNIT" "C_MAX_PERIOD#UNIT","Z#PERIOD"."COLLECTION_ID" "COLLECTION_ID","Z#PERIOD"."SN" "SN","Z#PERIOD"."SU" "SU" FROM "IBS"."Z#PERIOD"@"RFO_SNAP" "Z#PERIOD";
grant select on U1.V_RFO_Z#PERIOD to LOADDB;


