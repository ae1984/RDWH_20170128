AS
SELECT "ASSESSMENTCOMPANY"."ID" "ID","ASSESSMENTCOMPANY"."CODE" "CODE","ASSESSMENTCOMPANY"."ORD" "ORD","ASSESSMENTCOMPANY"."VALUE" "VALUE" FROM "DEBTCOLL"."ASSESSMENTCOMPANY"@"HCOL" "ASSESSMENTCOMPANY";
grant select on U1.M_HARD_COL_ASSESSMENTCOMPANY to LOADDB;


