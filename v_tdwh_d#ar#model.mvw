CREATE MATERIALIZED VIEW U1.V_TDWH_D#AR#MODEL
USING NO INDEX
REFRESH COMPLETE ON DEMAND
AS
SELECT "T"."ID" "ID","T"."CODE" "CODE","T"."RUS" "RUS","T"."ENG" "ENG","T"."KAZ" "KAZ","T"."RUS_DESC" "RUS_DESC","T"."ENG_DESC" "ENG_DESC","T"."KAZ_DESC" "KAZ_DESC","T"."D#AR#MARK" "D#AR#MARK","T"."DICT$D#MODEL" "DICT$D#MODEL" FROM "VIN"."D#AR#MODEL"@"TDWHTEST" "T";
grant select on U1.V_TDWH_D#AR#MODEL to LOADDB;
grant select on U1.V_TDWH_D#AR#MODEL to LOADER;


