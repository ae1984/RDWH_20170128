CREATE MATERIALIZED VIEW U1.M_HARD_COL_ASSETATTRIBUTE
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "ASSETATTRIBUTE"."ID" "ID","ASSETATTRIBUTE"."VALUE" "VALUE","ASSETATTRIBUTE"."ELEMENT_ID" "ELEMENT_ID","ASSETATTRIBUTE"."TYPE_ID" "TYPE_ID","ASSETATTRIBUTE"."ASSET_ID" "ASSET_ID","ASSETATTRIBUTE"."ATTRIBUTE_POSITION" "ATTRIBUTE_POSITION" FROM "DEBTCOLL"."ASSETATTRIBUTE"@"HCOL" "ASSETATTRIBUTE";
grant select on U1.M_HARD_COL_ASSETATTRIBUTE to LOADDB;


