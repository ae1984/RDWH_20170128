CREATE MATERIALIZED VIEW U1.M_HARD_COL_SESSION_AUD
BUILD DEFERRED
REFRESH FORCE ON DEMAND
AS
SELECT "SESSION_AUD"."ID" "ID","SESSION_AUD"."REV" "REV","SESSION_AUD"."REVTYPE" "REVTYPE","SESSION_AUD"."DONE" "DONE","SESSION_AUD"."FROMDATE" "FROMDATE","SESSION_AUD"."KIND" "KIND","SESSION_AUD"."THRUDATE" "THRUDATE","SESSION_AUD"."TYPE" "TYPE","SESSION_AUD"."COLLATERAL_ID" "COLLATERAL_ID" FROM "DEBTCOLL"."SESSION_AUD"@"HCOL" "SESSION_AUD";
grant select on U1.M_HARD_COL_SESSION_AUD to LOADDB;


