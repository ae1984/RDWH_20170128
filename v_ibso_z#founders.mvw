CREATE MATERIALIZED VIEW U1.V_IBSO_Z#FOUNDERS
BUILD DEFERRED
REFRESH COMPLETE ON DEMAND
AS
SELECT "Z#FOUNDERS"."ID" "ID","Z#FOUNDERS"."COLLECTION_ID" "COLLECTION_ID","Z#FOUNDERS"."C_FOUNDER" "C_FOUNDER","Z#FOUNDERS"."C_PERCENT" "C_PERCENT","Z#FOUNDERS"."C_IDENT_ER" "C_IDENT_ER","Z#FOUNDERS"."C_KAS_FOUNDER" "C_KAS_FOUNDER","Z#FOUNDERS"."C_KAS_REZIDENT" "C_KAS_REZIDENT","Z#FOUNDERS"."C_FOUNDER_PSEV" "C_FOUNDER_PSEV","Z#FOUNDERS"."C_CO_FOUNDERS" "C_CO_FOUNDERS","Z#FOUNDERS"."SN" "SN","Z#FOUNDERS"."SU" "SU" FROM "IBS"."Z#FOUNDERS"@"IBSO" "Z#FOUNDERS";
grant select on U1.V_IBSO_Z#FOUNDERS to LOADDB;


